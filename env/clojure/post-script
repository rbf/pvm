__devenv_print_and_run_command java -version || return $?
echo

# SOURCE: 16nov2021 https://github.com/clojure-emacs/cider/commit/37acd50
__devenv_print_and_run_command clojure --version || return $?
echo

__devenv_print_and_run_command lein --version || return $?
echo

devenv_aliases[java]='__devenv_docker_dev_img_run java'
devenv_aliases[clj]='__devenv_docker_dev_img_run clojure'
devenv_aliases[lein]='__devenv_docker_dev_img_run lein'
